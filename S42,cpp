#include <iostream>
using namespace std;

void sort(int arr[10]) {
    for (int i = 0; i < 9; i++) {
        for (int j = 0; j < 9; j++) {
            if (arr[j] < arr[j + 1]) {
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
}

int main() {
    int a[10] = {1, 8, 3, 6, 5, 4, 9, 2, 8, 12};
    
    sort(a);
    
    for (int i = 0; i < 10; i++) {
        cout << a[i] << " ";
    }
    
    return 0;
}
